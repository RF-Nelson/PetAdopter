<h2>Adoption Listings</h2>
<br>
<br>
<table>
  <tr>
    <strong>
      <td>Species</td>
      <td>Name</td>
      <td>Location</td>
    </strong>
  </tr>
  <% @listings.each do |listing| %>
    <tr>
      <td><%= listing.species %></td>
      <td><a href="/listings/<%= listing.id %>"><%= listing.pet_name %></a></td>
      <td><%= listing.location %></td>
    </tr>
  <% end %>
</table>
<br>
<a href="/listings/new">Create New Listing</a>
